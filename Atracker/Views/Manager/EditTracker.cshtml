@model Atracker.Models.ApplicationUser
@{
    Layout = "~/Views/Shared/_MangerLayout.cshtml";
}
<div class="custom-card">
    <h3>Edit Tracker: @Model.FullName</h3>
    <hr />
    <form asp-action="EditTracker">
        <input type="hidden" asp-for="Id" />
        <div class="form-group mb-2"><label asp-for="FullName"></label><input asp-for="FullName" class="form-control" /></div>
        <div class="form-group mb-2"><label asp-for="Email"></label><input asp-for="Email" class="form-control" /></div>
        <div class="form-group mb-3"><label asp-for="PhoneNumber"></label><input asp-for="PhoneNumber" class="form-control" /></div>
        <button type="submit" class="btn btn-primary">Save Changes</button>
    </form>
</div>